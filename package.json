{
  "name": "@metaxia/scriptures-source-stepbible-tagnt-tr",
  "version": "1.0.1",
  "description": "Textus Receptus (Greek) data from STEPBible TAGNT for @metaxia/scriptures. Complete TR with morphology and proper polytonic Greek.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./register": {
      "types": "./dist/register.d.ts",
      "import": "./dist/register.js"
    },
    "./metadata": "./data/stepbible-tagnt-tr/metadata.json",
    "./data/*": "./data/*",
    "./books/*": {
      "types": "./dist/books/*.d.ts",
      "import": "./dist/books/*.js"
    }
  },
  "files": [
    "dist",
    "data",
    "cache"
  ],
  "scripts": {
    "build": "tsc",
    "import": "tsx scripts/import.ts",
    "test": "vitest run",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "bible",
    "greek",
    "textus-receptus",
    "stepbible",
    "tagnt",
    "scriptures",
    "new-testament",
    "morphology"
  ],
  "author": "Benjamin Plunkert",
  "license": "(MIT AND CC-BY-4.0)",
  "repository": {
    "type": "git",
    "url": "https://github.com/metaxiamultimedia/scriptures-js-source-stepbible-tagnt-tr"
  },
  "bugs": {
    "url": "https://github.com/metaxiamultimedia/scriptures-js-source-stepbible-tagnt-tr/issues"
  },
  "homepage": "https://github.com/metaxiamultimedia/scriptures-js-source-stepbible-tagnt-tr#readme",
  "peerDependencies": {
    "@metaxia/scriptures-core": ">=0.1.2"
  },
  "devDependencies": {
    "@metaxia/scriptures-core": "git+https://github.com/metaxiamultimedia/scriptures-js-core.git#v0.1.2",
    "@types/node": "^22.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.0"
  }
}
